{
  "name": "lando-ui",
  "displayName": "Lando UI",
  "description": "Provides a vscode ui for the development tool lando",
  "version": "0.1.2",
  "publisher": "Jixabon",
  "icon": "icon.png",
  "homepage": "https://marketplace.visualstudio.com/items?itemName=jixabon.lando-ui",
  "keywords": [
    "lando",
    "website",
    "development"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jixabon/lando-ui.git"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "lando.start",
        "title": "Lando Start"
      },
      {
        "command": "lando.stop",
        "title": "Lando Stop"
      },
      {
        "command": "lando.poweroff",
        "title": "Lando Poweroff"
      },
      {
        "command": "lando.info-refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "lando.list-refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "lando.info-refreshNode",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "lando.list-refreshNode",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lando-ui",
          "title": "Lando UI",
          "icon": "media/lando.png"
        }
      ]
    },
    "views": {
      "lando-ui": [
        {
          "id": "lando-info",
          "name": "Lando Info"
        },
        {
          "id": "lando-list",
          "name": "Lando List"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "lando.start",
          "when": "view == lando-info"
        },
        {
          "command": "lando.stop",
          "when": "view == lando-info"
        },
        {
          "command": "lando.poweroff"
        },
        {
          "command": "lando.info-refresh",
          "when": "view == lando-info",
          "group": "navigation"
        },
        {
          "command": "lando.list-refresh",
          "when": "view == lando-list",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "tslint -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "ts-loader": "^6.2.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@types/yaml": "^1.2.0",
    "child_process": "^1.0.2",
    "jsonc-parser": "^0.4.2",
    "strip-ansi": "^5.2.0",
    "vscode-nls": "^4.1.1",
    "yaml": "^1.6.0"
  }
}
